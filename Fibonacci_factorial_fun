def fibo(n):
    if n == 0:
        return 0
    elif n == 1:
        return 1
    else:
        return fibo(n - 1) + fibo(n - 2)


def silnia(n):
    if n == 1:
        return 1
    elif n == 2:
        return 2
    else:
        return n * silnia(n-1)


index = 1
for x in range(1, 25):
    print(index, fibo(x))
    index = index + 1

for x in range(1, 20):
    print(silnia(x))

big = 121645100408832000
big_len = len(str(big))
